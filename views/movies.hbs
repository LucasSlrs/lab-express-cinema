<h1>IronHack Cinema</h1>
<h2>Click on the movie to check the showimes!</h2>
<div class="movies-page">
    {{#each movies}}
    <div>
        <div><img src="{{this.image}}" alt="movie image"></div>
        <div>
            <h3>{{this.title}}</h3>
        </div>
        <div><a class="second-a" href="/movies/{{_id}}">See more</a></div>
    </div>
    {{/each}}
</div>